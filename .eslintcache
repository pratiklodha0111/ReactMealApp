m \"./components/Cart/Cart\";\nimport CartProvider from \"./store/CartProvider\";\n\nfunction App() {\n  const [cartIsShown, setCartIsShown] = useState(false);\n  const showCartHandler = () => {\n    setCartIsShown(true);\n  };\n\n  const hideCartHandler = () => {\n    setCartIsShown(false);\n  };\n\n  return (\n    <CartProvider>\n      {cartIsShown && <Cart onClose={hideCartHandler} />}\n      <Header onShowCart={showCartHandler} />\n      <main>\n        <Meals />\n      </main>\n    </CartProvider>\n  );\n}\n\nexport default App;\n","/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Layout/Header.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Layout/HeaderCartButton.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Cart/CartIcon.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Meals/Meals.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Meals/AvailableMeals.js",["80"],"import React, { useEffect, useState } from \"react\";\nimport classes from \"./AvailableMeals.module.css\";\nimport Card from \".././UI/Card\";\nimport MealItem from \"./MealItem/MealItem\";\nconst DUMMY_MEALS = [\n  {\n    id: \"m1\",\n    name: \"Sushi\",\n    description: \"Finest fish and veggies\",\n    price: 22.99,\n  },\n  {\n    id: \"m2\",\n    name: \"Schnitzel\",\n    description: \"A german specialty!\",\n    price: 16.5,\n  },\n  {\n    id: \"m3\",\n    name: \"Barbecue Burger\",\n    description: \"American, raw, meaty\",\n    price: 12.99,\n  },\n  {\n    id: \"m4\",\n    name: \"Green Bowl\",\n    description: \"Healthy...and green...\",\n    price: 18.99,\n  },\n];\nconst AvailableMeals = () => {\n  const [meals, setMeals] = useState([]);\n\n  const [isLoading, setIsLoading] = useState(true);\n  const [httpError, setHttpError] = useState();\n\n  useEffect(() => {\n    const fetchMeals = async () => {\n      const response = await fetch(\n        \"https://react-food-delivery-fb552-default-rtdb.firebaseio.com/meals.json\"\n      );\n      if (!response.ok) {\n        throw new Error(\"Something Went Error\");\n      }\n      const responseData = await response.json();\n\n      const loadedMeals = [];\n\n      for (const key in responseData) {\n        loadedMeals.push({\n          id: key,\n          name: responseData[key].name,\n          description: responseData[key].description,\n          price: responseData[key].price,\n        });\n      }\n      setMeals(loadedMeals);\n      setIsLoading(false);\n    };\n    fetchMeals().catch((error) => {\n      setIsLoading(false);\n      setHttpError(error.message);\n    });\n  }, []);\n\n  if (isLoading) {\n    return (\n      <section className={classes.MealsLoading}>\n        <p>Loading ....</p>\n      </section>\n    );\n  }\n\n  if (httpError) {\n    return (\n      <section className={classes.MealsError}>\n        <p>{httpError}</p>\n      </section>\n    );\n  }\n  const mealList = meals.map((meal) => (\n    <MealItem\n      id={meal.id}\n      key={meal.id}\n      name={meal.name}\n      description={meal.description}\n      price={meal.price}\n    ></MealItem>\n  ));\n  return (\n    <section className={classes.meals}>\n      <Card>\n        <ul>{mealList}</ul>\n      </Card>\n    </section>\n  );\n};\n\nexport default AvailableMeals;\n","/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Meals/MealsSummary.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/UI/Card.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Meals/MealItem/MealItem.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/UI/Input.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Meals/MealItem/MealItemForm.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/Cart/Cart.js",[],"/home/sunbeam/dac/notes/reactJS/workspace/Food Order/src/components/UI/Modal.js",[